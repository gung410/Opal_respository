<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <!-- !!!! ======================================================================================== !!!! -->
  <!-- !!!! Remember to change the build for this file: File must be marked Content + Copy to Output !!!! -->
  <!-- !!!! ======================================================================================== !!!! -->
  <appSettings>
    <add key="serviceUsername" value="staginguser" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="servicePassword" value="stagingpassword" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="OwnerId" value="15" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="SiteId" value="2" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="HDID" value="2" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="MasterRedisConfiguration" value="coe-mssql03:6431,allowAdmin=true" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="SlaveRedisConfiguration" value="127.0.0.1:6431,allowAdmin=true" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="DisableGetDynamicProperties" value="True" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="DisableCheckAccessRule" value="True" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="UserTypeEmployee" value="49" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
    <add key="CurrentUserId" value="34529" xdt:Transform="Replace"  xdt:Locator="Match(key)"/>
  </appSettings>

  <connectionStrings>
    <add name="ErrorLogging" connectionString="Data Source=coe-mssql03;Initial Catalog=staging-competence-no-at6q;Persist Security Info=True;User ID=atadmin; Password=morty" providerName="System.Data.SqlClient" xdt:Transform="Replace"  xdt:Locator="Match(name)"/>
    <add name="AT6QDBTemplate" connectionString="Data Source=coe-mssql03;Initial Catalog=staging-competence-no-at6q; User ID=atadmin;Password=morty" providerName="System.Data.SqlClient" xdt:Transform="Replace"  xdt:Locator="Match(name)"/>
  </connectionStrings>

  <dataCacheClient xdt:Transform="Remove" />
  <dataCacheClient xdt:Transform="InsertIfMissing">
    <hosts>
      <host name="COE-CACHE01.coe.osl.basefarm.net" cachePort="22233"/>
      <host name="COE-CACHE02.coe.osl.basefarm.net" cachePort="22233"/>
      <host name="COE-CACHE03.coe.osl.basefarm.net" cachePort="22233"/>
    </hosts>
  </dataCacheClient>

  <elmah>
    <errorLog xdt:Transform="Remove" />
    <errorLog type=
    "Elmah.SqlErrorLog, Elmah" connectionStringName="ErrorLogging" applicationName="staging-competence-no-api-organization" xdt:Transform="InsertIfMissing" />
  </elmah>

  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>

  <log4net xdt:Transform="Remove" />
  <log4net xdt:Transform="InsertIfMissing">
    <root>
      <level value="INFO" />
      <appender-ref ref="RollingLogFileAppender" />
      <appender-ref ref="ElasticSearchAppender" />
    </root>
    <appender name="ElasticSearchAppender" type="log4stash.ElasticSearchAppender, log4stash">
      <Server>log-receiver.conexus.no</Server>
      <Port>80</Port>
      <IndexName>staging-app-log%{+yyyy-MM-dd}</IndexName>
      <IndexType>LogEvent</IndexType>
      <Bulksize>100</Bulksize>
      <BulkIdleTimeout>5000</BulkIdleTimeout>
      <IndexAsync>False</IndexAsync>
      <evaluator type="log4net.Core.LevelEvaluator">
        <threshold value="ERROR" />
      </evaluator>
      <!-- for more information read about log4net.Core.FixFlags -->
      <FixedFields>Partial</FixedFields>
      <ElasticFilters>
        <Add>
          <Key>sitename</Key>
          <Value>staging-competence-no-api-organization</Value>
        </Add>
      </ElasticFilters>
    </appender>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="C:\applogs\staging-competence-no-api-organization-log-file.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="100" />
      <maximumFileSize value="2MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{RequestId}] [%property{CorrelationId}] - %message%newline" />
      </layout>
    </appender>
  </log4net>
</configuration>