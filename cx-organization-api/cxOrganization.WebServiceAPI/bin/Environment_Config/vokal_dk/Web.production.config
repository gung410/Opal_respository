  <connectionStrings>
    <add name="ErrorLogging" connectionString="Data Source=coe-mssql02;Initial Catalog=staging-engage-dk-elmah;Persist Security Info=True;User ID=atadmin; Password=morty" providerName="System.Data.SqlClient" xdt:Transform="Replace"  xdt:Locator="Match(name)"/>
    <add name="AT6QDBTemplate" connectionString="Data Source=coe-mssql04;Initial Catalog=AT6Q_VOKAL_DK; User ID=atadmin;Password=morty" providerName="System.Data.SqlClient" xdt:Transform="Replace"  xdt:Locator="Match(name)"/>
  </connectionStrings>
  
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  
  <elmah>
    <errorLog xdt:Transform="Remove" />
    <errorLog type=
    "Elmah.SqlErrorLog, Elmah" connectionStringName="ErrorLogging" applicationName="production-vokal-dk-api-organization" xdt:Transform="InsertIfMissing" />
  </elmah>

  <log4net xdt:Transform="Remove" />
  <log4net xdt:Transform="InsertIfMissing">
    <root>
      <level value="ERROR" />
      <appender-ref ref="RollingLogFileAppender" />
      <appender-ref ref="ElasticSearchAppender" />
    </root>
    <appender name="ElasticSearchAppender" type="log4stash.ElasticSearchAppender, log4stash">
      <Server>log-receiver.conexus.no</Server>
      <Port>80</Port>
      <IndexName>production-app-%{+yyyy-MM-dd}</IndexName>
      <IndexType>LogEvent</IndexType>
      <Bulksize>100</Bulksize>
      <BulkIdleTimeout>30000</BulkIdleTimeout>
      <IndexAsync>False</IndexAsync>
      <evaluator type="log4net.Core.LevelEvaluator">
        <threshold value="ERROR" />
      </evaluator>
      <!-- for more information read about log4net.Core.FixFlags -->
      <FixedFields>Partial</FixedFields>
      <ElasticFilters>
        <Add>
          <Key>sitename</Key>
          <Value>production-vokal-dk-api-organization</Value>
        </Add>
      </ElasticFilters>
    </appender>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="C:\inetpub\logs\LogFiles\Log4Net\production-vokal-no-api-organization-log.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="100" />
      <maximumFileSize value="2MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{RequestId}] [%property{CorrelationId}] - %message%newline" />
      </layout>
    </appender>
  </log4net>
</configuration>