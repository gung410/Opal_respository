{
  "name": "cx-angular-common",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "rimraf dist\\cx-angular-common && ng build CxAngularCommon && npm run build-styles:lib",
    "build-styles:lib": "copyfiles -f ./projects/cx-angular-common/src/lib/styles/*.scss ./dist/cx-angular-common/lib/styles/ && scss-bundle -c build-styles-lib.scss-bundle.config.json && npm run scss-to-css:lib",
    "scss-to-css:lib": "node-sass ./dist/cx-angular-common/lib/styles/_styles.scss ./dist/cx-angular-common/lib/styles/_styles.css",
    "publish:lib": "npm run build:lib && cd dist\\cx-angular-common && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
