{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cx-organization-spa": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/cx-organization-spa",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/silent-refresh.html",
              "src/firebase-messaging-sw.js",
              "src/firebase-messaging-sw.production.js",
              "src/manifest.json"
            ],
            "styles": [
              "node_modules/jquery-ui/themes/base/all.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/select2/dist/css/select2.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/@conexus/cx-angular-common/lib/styles/_styles.scss",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/select2/dist/js/select2.min.js",
              "node_modules/jquery-ui/ui/widgets/datepicker.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ],
            "es5BrowserSupport": true,
            "preserveSymlinks": true
          },
          "configurations": {
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/build_settings/moe/development/environment.ts"
                },
                {
                  "replace": "src/firebase-messaging-sw.js",
                  "with": "src/build_settings/moe/development/firebase-messaging-sw.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb"
                }
              ]
            },
            "uat-next": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/build_settings/moe/uat-next/environment.ts"
                },
                {
                  "replace": "src/firebase-messaging-sw.js",
                  "with": "src/build_settings/moe/uat-next/firebase-messaging-sw.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb"
                }
              ]
            },
            "systemtest": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/build_settings/moe/systemtest/environment.ts"
                },
                {
                  "replace": "src/firebase-messaging-sw.js",
                  "with": "src/build_settings/moe/systemtest/firebase-messaging-sw.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb"
                }
              ]
            },
            "uat": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/build_settings/moe/uat/environment.ts"
                },
                {
                  "replace": "src/firebase-messaging-sw.js",
                  "with": "src/build_settings/moe/uat/firebase-messaging-sw.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb"
                }
              ]
            },
            "prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/build_settings/moe/production/environment.ts"
                },
                {
                  "replace": "src/firebase-messaging-sw.js",
                  "with": "src/build_settings/moe/production/firebase-messaging-sw.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/build_settings/moe/production/environment.ts"
                },
                {
                  "replace": "src/firebase-messaging-sw.js",
                  "with": "src/build_settings/moe/production/firebase-messaging-sw.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb"
                }
              ]
            },
            "pre-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/build_settings/moe/pre-prod/environment.ts"
                },
                {
                  "replace": "src/firebase-messaging-sw.js",
                  "with": "src/build_settings/moe/pre-prod/firebase-messaging-sw.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cx-organization-spa:build",
            "port": 4202
          },
          "configurations": {
            "production": {
              "browserTarget": "cx-organization-spa:build:production"
            },
            "systemtest": {
              "browserTarget": "cx-organization-spa:build:systemtest"
            },
            "development": {
              "browserTarget": "cx-organization-spa:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cx-organization-spa:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": ["src/styles.scss"],
            "scripts": [],
            "assets": ["src/favicon.ico", "src/assets"],
            "preserveSymlinks": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "cx-organization-spa-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cx-organization-spa:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cx-organization-spa:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "cx-organization-spa",
  "cli": {
    "analytics": false
  }
}
